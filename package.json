{
  "name": "@origyn-sa/mintjs",
  "version": "1.0.0-beta.0",
  "description": "JavaScript library for staging and minting Origyn NFTs",
  "keywords": [
    "mint",
    "nft",
    "origyn"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
    "compile": "tsc -b tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "test": "jest --config jestconfig.json",
    "version": "npm run format && git add -A src"
  },
  "dependencies": {
    "@dfinity/agent": "^0.13.1",
    "@dfinity/auth-client": "^0.13.2",
    "@dfinity/candid": "^0.13.1",
    "@dfinity/identity": "^0.13.1",
    "@dfinity/principal": "^0.13.1",
    "bip39": "^3.0.4",
    "buffer-crc32": "^0.2.13",
    "cross-fetch": "^3.1.5",
    "crypto-js": "^4.1.1",
    "hdkey": "^2.0.1",
    "mime-types": "^2.1.35",
    "pem-file": "^1.0.1",
    "sha256": "^0.2.0",
    "ts-results": "^3.3.0"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "jest": "^28.1.3",
    "json-bigint": "^1.0.0",
    "node-fetch": "^2.6.7",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ORIGYN-SA/mintjs.git"
  },
  "files": [
    "lib/**/*"
  ],
  "author": "Origyn",
  "bugs": {
    "url": "https://github.com/ORIGYN-SA/mintjs/issues"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "homepage": "https://github.com/ORIGYN-SA/mintjs#readme"
}
