{
  "name": "mintjs",
  "version": "1.0.0-beta.0",
  "description": "JavaScript library for staging and minting Origyn NFTs",
  "keywords": [
    "mint",
    "nft",
    "origyn"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepublishOnly": "npm test && npm run lint",
    "prepare": "npm run build",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ORIGYN-SA/mintjs.git"
  },
  "files": [
    "lib/**/*"
  ],
  "author": "Origyn",
  "bugs": {
    "url": "https://github.com/ORIGYN-SA/mintjs/issues"
  },
  "homepage": "https://github.com/ORIGYN-SA/mintjs#readme"
}
